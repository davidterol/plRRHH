    "@payloadcms/richtext-lexical": "latest",
    "@radix-ui/react-dialog": "^1.1.2",
    "@types/luxon": "^3.4.2",
    "@types/react-big-calendar": "^1.16.0",
    "cross-env": "^7.0.3",
    "fs": "^0.0.1-security",
    "graphql": "^16.8.1",
    "next": "15.0.0",
    "payload": "latest",
    "payload-plugin-masquerade": "^1.0.0",
    "prop-types": "^15.8.1",
